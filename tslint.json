{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "jsx-no-lambda": false,
    "interface-name": false,
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-console": false,
    "member-access": [
      false
    ],
    "jsdoc-format": false,
    "object-literal-shorthand": false,
    "no-shadowed-variable": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
